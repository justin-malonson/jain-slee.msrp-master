<?xml version="1.0"?>

<!DOCTYPE event-jar PUBLIC
                "-//Sun Microsystems, Inc.//DTD JAIN SLEE Event 1.1//EN"
                "http://java.sun.com/dtd/slee-event-jar_1_1.dtd">

<event-jar>

    <library-ref>
        <library-name>msrp-chatroom-library</library-name>
        <library-vendor>org.mobicents.examples</library-vendor>
        <library-version>1.0</library-version>
    </library-ref>

    <event-definition>
        <event-type-name>org.mobicents.msrp.events.chat</event-type-name>
        <event-type-vendor>org.mobicents.examples</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.slee.example.msrp.events.ChatEvent</event-class-name>
    </event-definition>
    <event-definition>
        <event-type-name>org.mobicents.msrp.events.legDisconnected</event-type-name>
        <event-type-vendor>org.mobicents.examples</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.slee.example.msrp.events.LegDisconnectedEvent</event-class-name>
    </event-definition>

    <!--  chat control events -->
    <event-definition>
        <event-type-name>org.mobicents.msrp.events.createMixer</event-type-name>
        <event-type-vendor>org.mobicents.examples</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.slee.example.msrp.events.ChatEvent</event-class-name>
    </event-definition>
    <event-definition>
        <event-type-name>org.mobicents.msrp.events.createComplete</event-type-name>
        <event-type-vendor>org.mobicents.examples</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.slee.example.msrp.events.ChatEvent</event-class-name>
    </event-definition>
    <event-definition>
        <event-type-name>org.mobicents.msrp.events.createMedia</event-type-name>
        <event-type-vendor>org.mobicents.examples</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.slee.example.msrp.events.ChatEvent</event-class-name>
    </event-definition>
    <event-definition>
        <event-type-name>org.mobicents.msrp.events.mediaReleased</event-type-name>
        <event-type-vendor>org.mobicents.examples</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.slee.example.msrp.events.ChatEvent</event-class-name>
    </event-definition>
    <event-definition>
        <event-type-name>org.mobicents.msrp.events.updateParticipant</event-type-name>
        <event-type-vendor>org.mobicents.examples</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.slee.example.msrp.events.ChatEvent</event-class-name>
    </event-definition>
    <event-definition>
        <event-type-name>org.mobicents.msrp.events.updateConference</event-type-name>
        <event-type-vendor>org.mobicents.examples</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.slee.example.msrp.events.ChatEvent</event-class-name>
    </event-definition>
</event-jar>
